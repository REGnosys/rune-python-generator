{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "prConcurrentLimit": 10,
  "prHourlyLimit": 20,
  "packageRules": [
    {
      "groupName": "Rosetta DSL packages",
      "matchPackageNames": [
        "com.regnosys.rosetta",
        "com.regnosys.rosetta.lib",
        "com.regnosys.rosetta.tests"
      ],
      "enabled": true
    },
    {
      "matchPackagePatterns": [".*"],
      "enabled": false
    }
  ]
}
